<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_home_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.graduationproject.grad_project.view.admin.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_requestAdminFragment"
            app:destination="@id/requestAdminFragment" />
        <action
            android:id="@+id/action_homeFragment_to_votingAdminFragment"
            app:destination="@id/votingAdminFragment" />
    </fragment>
    <fragment
        android:id="@+id/announcementsFragment"
        android:name="com.graduationproject.grad_project.view.admin.AnnouncementsFragment"
        android:label="AnnouncementsFragment" >
        <action
            android:id="@+id/action_announcementsFragment_to_addAnnouncementFragment"
            app:destination="@id/addAnnouncementFragment" />
        <action
            android:id="@+id/action_announcementsFragment_to_showNotificationDialogFragment2"
            app:destination="@id/showNotificationDialogFragment2" />
    </fragment>
    <fragment
        android:id="@+id/expendituresFragment"
        android:name="com.graduationproject.grad_project.view.admin.ExpendituresFragment"
        android:label="ExpendituresFragment" >
        <action
            android:id="@+id/action_expendituresFragment_to_addExpendituresFragment"
            app:destination="@id/addExpendituresFragment" />
    </fragment>
    <fragment
        android:id="@+id/addAnnouncementFragment"
        android:name="com.graduationproject.grad_project.view.admin.AddAnnouncementFragment"
        android:label="AddAnnouncementFragment" >
        <action
            android:id="@+id/action_addAnnouncementFragment_to_announcementsFragment"
            app:destination="@id/announcementsFragment"
            app:popUpTo="@id/addAnnouncementFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/notificationsAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.NotificationsAdminFragment"
        android:label="fragment_notifications_admin"
        tools:layout="@layout/fragment_notifications_admin" >
        <action
            android:id="@+id/action_notificationsAdminFragment_to_showNotificationDialogFragment2"
            app:destination="@id/showNotificationDialogFragment2" />
    </fragment>
    <fragment
        android:id="@+id/votingAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.VotingAdminFragment"
        android:label="fragment_voting_admin"
        tools:layout="@layout/fragment_voting_admin" >
        <action
            android:id="@+id/action_votingAdminFragment_to_addVotingAdminFragment"
            app:destination="@id/addVotingAdminFragment" />
    </fragment>
    <fragment
        android:id="@+id/servicesAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.ServicesAdminFragment"
        android:label="fragment_services_admin"
        tools:layout="@layout/fragment_services_admin" >
        <action
            android:id="@+id/action_servicesAdminFragment_to_servicesAdminDialogFragment"
            app:destination="@id/servicesAdminDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/meetingAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.MeetingAdminFragment"
        android:label="fragment_meeting_admin"
        tools:layout="@layout/fragment_meeting_admin" />
    <fragment
        android:id="@+id/pendingApprovalsAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.PendingApprovalsAdminFragment"
        android:label="fragment_pending_approvals_admin"
        tools:layout="@layout/fragment_pending_approvals_admin" />
    <fragment
        android:id="@+id/settingsAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.SettingsAdminFragment"
        android:label="fragment_settings_admin"
        tools:layout="@layout/fragment_settings_admin" >
        <action
            android:id="@+id/action_settingsAdminFragment_to_settingsNameFragment"
            app:destination="@id/settingsNameFragment" />
        <action
            android:id="@+id/action_settingsAdminFragment_to_settingsPhoneFragment"
            app:destination="@id/settingsPhoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/addVotingAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.AddVotingAdminFragment"
        android:label="fragment_add_voting_admin"
        tools:layout="@layout/fragment_add_voting_admin" >
        <action
            android:id="@+id/action_addVotingAdminFragment_to_votingAdminFragment"
            app:destination="@id/votingAdminFragment"
            app:popUpTo="@id/addVotingAdminFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/residentsListFragment"
        android:name="com.graduationproject.grad_project.view.admin.ResidentsListFragment"
        android:label="ResidentsListFragment" />
    <fragment
        android:id="@+id/addExpendituresFragment"
        android:name="com.graduationproject.grad_project.view.admin.AddExpendituresFragment"
        android:label="AddExpendituresFragment" >
        <action
            android:id="@+id/action_addExpendituresFragment_to_expendituresFragment"
            app:destination="@id/expendituresFragment"
            app:popUpTo="@id/addExpendituresFragment"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/settingsNameFragment"
        android:name="com.graduationproject.grad_project.view.SettingsNameFragment"
        android:label="fragment_settings_name"
        tools:layout="@layout/fragment_settings_name" >
        <action
            android:id="@+id/action_settingsNameFragment_to_settingsAdminFragment"
            app:destination="@id/settingsAdminFragment" />
        <argument
            android:name="fullName"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/settingsPhoneFragment"
        android:name="com.graduationproject.grad_project.view.SettingsPhoneFragment"
        android:label="fragment_settings_phone"
        tools:layout="@layout/fragment_settings_phone" >
        <action
            android:id="@+id/action_settingsPhoneFragment_to_settingsAdminFragment"
            app:destination="@id/settingsAdminFragment" />
    </fragment>
    <dialog
        android:id="@+id/servicesAdminDialogFragment"
        android:name="com.graduationproject.grad_project.view.admin.ServicesAdminDialogFragment"
        android:label="fragment_services_admin_dialog"
        tools:layout="@layout/fragment_services_admin_dialog" >
        <action
            android:id="@+id/action_servicesAdminDialogFragment_to_servicesAdminFragment"
            app:destination="@id/servicesAdminFragment"
            app:popUpTo="@id/servicesAdminDialogFragment"
            app:popUpToInclusive="true" />
    </dialog>
    <dialog
        android:id="@+id/showNotificationDialogFragment2"
        android:name="com.graduationproject.grad_project.view.resident.dialogs.ShowNotificationDialogFragment"
        android:label="fragment_show_notification_dialog"
        tools:layout="@layout/fragment_show_notification_dialog" >
        <argument
            android:name="downloadUri"
            app:argType="string" />
        <argument
            android:name="title"
            app:argType="string" />
        <argument
            android:name="content"
            app:argType="string" />
    </dialog>
    <dialog
        android:id="@+id/showRequestInfoDialogFragment"
        android:name="com.graduationproject.grad_project.view.admin.dialogs.ShowRequestInfoDialogFragment"
        android:label="fragment_show_request_info_dialog"
        tools:layout="@layout/fragment_show_request_info_dialog" >
        <action
            android:id="@+id/action_showRequestInfoDialogFragment_to_requestAdminFragment"
            app:destination="@id/requestAdminFragment"
            app:popUpTo="@id/showRequestInfoDialogFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="downloadUri"
            app:argType="string" />
        <argument
            android:name="title"
            app:argType="string" />
        <argument
            android:name="content"
            app:argType="string" />
        <argument
            android:name="type"
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/requestAdminFragment"
        android:name="com.graduationproject.grad_project.view.admin.RequestAdminFragment"
        android:label="fragment_request_admin"
        tools:layout="@layout/fragment_request_admin" >
        <action
            android:id="@+id/action_requestAdminFragment_to_showRequestInfoDialogFragment"
            app:destination="@id/showRequestInfoDialogFragment" />
    </fragment>
</navigation>